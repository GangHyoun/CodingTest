	// 5번 : 공 넣기
	int N, M;
	cin >> N >> M;
	vector<int> num(100);
	int a, b, c;

	for (int i = 0; i < M; i++) {
		cin >> a >> b >> c;
		for (int j = a; j <= b; j++) {
			num[j] = c;
		}
	}

	for (int i = 1; i <= N; i++) {
		cout << num[i] << " ";
	}
 
	return 0;
