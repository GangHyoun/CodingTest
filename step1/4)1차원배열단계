	// 10번 : 평균
	int N= 0;
	cin >> N;
	vector<float> score(N);

	for (int i = 0; i < N; i++) {
		cin >> score[i];
	}

	float max = score[0];

	for (int i = 0; i < N; i++) {
		if (max < score[i]) {
			max = score[i];
		}
	}

	for (int i = 0; i < N; i++) {
		score[i] = score[i] / max * 100.0;
	}

	float sum= 0;
	for (int i = 0; i < N; i++) {
		sum += score[i];
	}

	cout << fixed;
	cout.precision(3);
	cout << sum / N;

	return 0;
